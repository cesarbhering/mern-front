{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "trybe-frontend"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-max-depth": 0
    }
}
